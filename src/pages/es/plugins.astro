---
import Layout from '@/layouts/Layout.astro'
import VuePlugins from '@/vue-pages/plugins.vue'

const title = [Astro.locals.runtimeConfig.public.brand, Astro.locals.translations['plugins'][Astro.locals.locale]].join(' | ')
const description = Astro.locals.translations['plugins_description'][Astro.locals.locale]

const content = { title, description }
---

<Layout content={content}><VuePlugins locale={Astro.locals.locale} client:load /></Layout>
