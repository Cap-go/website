---
import Layout from '@/layouts/Layout.astro'
import VueTopflutterapp from '@/vue-pages/top_flutter_app.vue'

const title = [Astro.locals.runtimeConfig.public.brand, Astro.locals.translations['top_flutter_apps_title'][Astro.locals.locale]].join(' | ')
const description = Astro.locals.translations['top_flutter_apps'][Astro.locals.locale]

const content = { title, description }
---

<Layout content={content}><VueTopflutterapp locale={Astro.locals.locale} client:load /></Layout>
