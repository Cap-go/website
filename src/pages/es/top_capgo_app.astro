---
import Layout from '@/layouts/Layout.astro'
import VueTopcapgoapp from '@/vue-pages/top_capgo_app.vue'

const title = [Astro.locals.runtimeConfig.public.brand, Astro.locals.translations['top_capgo_apps_title'][Astro.locals.locale]].join(' | ')
const description = Astro.locals.translations['top_capgo_apps'][Astro.locals.locale]

const content = { title, description }
---

<Layout content={content}><VueTopcapgoapp locale={Astro.locals.locale} client:load /></Layout>
