---
import Layout from '@/layouts/Layout.astro'
import VueEula from '@/vue-pages/eula.vue'
import * as m from "../paraglide/messages.js"

const title = [Astro.locals.runtimeConfig.public.brand, m.eula()].join(' | ')
const description = m.read_our_eula()

const content = { title, description }
---

<Layout content={content}><VueEula locale={Astro.locals.locale} client:load /></Layout>
