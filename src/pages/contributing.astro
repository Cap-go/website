---
import Layout from '@/layouts/Layout.astro'
import VueContributing from '@/vue-pages/contributing.vue'
import * as m from '../paraglide/messages.js'

const title = [Astro.locals.runtimeConfig.public.brand, m.contributing({}, { locale: Astro.locals.locale })].join(' | ')
const description = m.contributing_description({}, { locale: Astro.locals.locale })

const content = { title, description }
---

<Layout content={content}><VueContributing locale={Astro.locals.locale} client:load /></Layout>
