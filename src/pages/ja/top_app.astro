---
import Layout from '@/layouts/Layout.astro'
import VueTopapp from '@/vue-pages/top_app.vue'

const title = [Astro.locals.runtimeConfig.public.brand, Astro.locals.translations['top_apps_by_framework'][Astro.locals.locale]].join(' | ')
const description = Astro.locals.translations['top_100_app_using_different_framworks_on_android_store'][Astro.locals.locale]

const content = { title, description }
---

<Layout content={content}><VueTopapp locale={Astro.locals.locale} client:load /></Layout>
