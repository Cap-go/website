---
import Layout from '@/layouts/Layout.astro'
import VueTopflutterapp from '@/vue-pages/top_flutter_app.vue'
import * as m from '@/paraglide/messages'

const title = [Astro.locals.runtimeConfig.public.brand, m.top_flutter_apps_title({}, { locale: Astro.locals.locale })].join(' | ')
const description = m.top_flutter_apps({}, { locale: Astro.locals.locale })

const content = { title, description }
---

<Layout content={content}><VueTopflutterapp locale={Astro.locals.locale} client:load /></Layout>
