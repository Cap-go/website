---
import Layout from '@/layouts/Layout.astro'
import VueTopreactnativeapp from '@/vue-pages/top_react_native_app.vue'
import * as m from "../paraglide/messages.js"

const title = [Astro.locals.runtimeConfig.public.brand, m.top_react_native_apps_title()].join(' | ')
const description = m.top_react_native_apps()

const content = { title, description }
---

<Layout content={content}><VueTopreactnativeapp locale={Astro.locals.locale} client:load /></Layout>
