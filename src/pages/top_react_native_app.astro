---
import Layout from '@/layouts/Layout.astro'
import { defaultLocale } from '@/services/locale'
import VueTopreactnativeapp from '@/vue-pages/top_react_native_app.vue'

const title = 'Capgo | Top React Native apps'
const description = 'List of top 100 app using React Native on android store'

const content = {
  description,
  title,
}
---

<Layout content={content}><VueTopreactnativeapp locale={Astro.currentLocale || defaultLocale} client:load /></Layout>
