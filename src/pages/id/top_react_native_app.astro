---
import Layout from '@/layouts/Layout.astro'
import VueTopreactnativeapp from '@/vue-pages/top_react_native_app.vue'

const title = [Astro.locals.runtimeConfig.public.brand, Astro.locals.translations['top_react_native_apps_title'][Astro.locals.locale]].join(' | ')
const description = Astro.locals.translations['top_react_native_apps'][Astro.locals.locale]

const content = { title, description }
---

<Layout content={content}><VueTopreactnativeapp locale={Astro.locals.locale} client:load /></Layout>
