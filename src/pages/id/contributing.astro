---
import Layout from '@/layouts/Layout.astro'
import VueContributing from '@/vue-pages/contributing.vue'

const title = [Astro.locals.runtimeConfig.public.brand, Astro.locals.translations['contributing'][Astro.locals.locale]].join(' | ')
const description = Astro.locals.translations['contributing_description'][Astro.locals.locale]

const content = { title, description }
---

<Layout content={content}><VueContributing locale={Astro.locals.locale} client:load /></Layout>
