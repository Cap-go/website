---
import VueDataPolicy from '@/vue-pages/data_policy.vue'
import Layout from '@/layouts/Layout.astro'
import * as m from '@/paraglide/messages'

const title = [Astro.locals.runtimeConfig.public.brand, m.dp({}, { locale: Astro.locals.locale })].join(' | ')
const description = m.dp({}, { locale: Astro.locals.locale })

const content = { title, description }
---

<Layout content={content}><VueDataPolicy locale={Astro.locals.locale} client:load /></Layout>
