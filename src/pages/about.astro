---
import Layout from '@/layouts/Layout.astro'
import VueAbout from '@/vue-pages/about.vue'
import * as m from '@/paraglide/messages'

const title = [Astro.locals.runtimeConfig.public.brand, m.about_capgo({}, { locale: Astro.locals.locale })].join(' | ')
const description = m.about_capgo({}, { locale: Astro.locals.locale })

const content = {
  title,
  description,
  image: `${Astro.locals.runtimeConfig.public.baseUrl}/app_demo.webp`,
}
---

<Layout content={content}><VueAbout locale={Astro.locals.locale} client:load /></Layout>
