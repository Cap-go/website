---
import Layout from '@/layouts/Layout.astro'
import VueTopcordovaapp from '@/vue-pages/top_cordova_app.vue'
import * as m from '../paraglide/messages.js'

const title = [Astro.locals.runtimeConfig.public.brand, m.top_cordova_apps_title({}, { locale: Astro.locals.locale })].join(' | ')
const description = m.top_cordova_apps({}, { locale: Astro.locals.locale })

const content = { title, description }
---

<Layout content={content}><VueTopcordovaapp locale={Astro.locals.locale} client:load /></Layout>
