---
import Layout from '@/layouts/Layout.astro'
import { defaultLocale, type Locales } from '@/services/locale'
import VueTopcordovaapp from '@/vue-pages/top_cordova_app.vue'

const title = 'Capgo | Top Cordova apps'
const description = 'List of top 100 app using Cordova on android store'

const content = {
  description,
  title,
}
---

<Layout content={content}><VueTopcordovaapp locale={(Astro.currentLocale || defaultLocale) as Locales} client:load /></Layout>
