---
import Layout from '@/layouts/Layout.astro'
import VueTopcapgoapp from '@/vue-pages/top_capgo_app.vue'
import * as m from "../paraglide/messages.js"

const title = [Astro.locals.runtimeConfig.public.brand, m.top_capgo_apps_title()].join(' | ')
const description = m.top_capgo_apps()

const content = { title, description }
---

<Layout content={content}><VueTopcapgoapp locale={Astro.locals.locale} client:load /></Layout>
