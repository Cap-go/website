---
import Layout from '@/layouts/Layout.astro'
import VueTopnativescriptapp from '@/vue-pages/top_native_script_app.vue'

const title = [Astro.locals.runtimeConfig.public.brand, Astro.locals.translations['top_native_script_apps_title'][Astro.locals.locale]].join(' | ')
const description = Astro.locals.translations['top_native_script_apps'][Astro.locals.locale]

const content = { title, description }
---

<Layout content={content}><VueTopnativescriptapp locale={Astro.locals.locale} client:load /></Layout>
