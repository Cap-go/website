---
import Layout from '@/layouts/Layout.astro'
import VueTopnativescriptapp from '@/vue-pages/top_native_script_app.vue'
import * as m from "../paraglide/messages.js"

const title = [Astro.locals.runtimeConfig.public.brand, m.top_native_script_apps_title()].join(' | ')
const description = m.top_native_script_apps()

const content = { title, description }
---

<Layout content={content}><VueTopnativescriptapp locale={Astro.locals.locale} client:load /></Layout>
