---
import Layout from '@/layouts/Layout.astro'
import VueTopnativescriptapp from '@/vue-pages/top_native_script_app.vue'
import * as m from '@/paraglide/messages'

const title = [Astro.locals.runtimeConfig.public.brand, m.top_native_script_apps_title({}, { locale: Astro.locals.locale })].join(' | ')
const description = m.top_native_script_apps({}, { locale: Astro.locals.locale })

const content = { title, description }
---

<Layout content={content}><VueTopnativescriptapp locale={Astro.locals.locale} client:load /></Layout>
