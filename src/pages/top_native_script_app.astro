---
import Layout from '@/layouts/Layout.astro'
import { defaultLocale, type Locales } from '@/services/locale'
import VueTopnativescriptapp from '@/vue-pages/top_native_script_app.vue'

const title = 'Capgo | Top Native Script apps'
const description = 'List of top 100 app using Native Script on android store'

const content = {
  description,
  title,
}
---

<Layout content={content}><VueTopnativescriptapp locale={(Astro.currentLocale || defaultLocale) as Locales} client:load /></Layout>
