---
import Layout from '@/layouts/Layout.astro'
import VuePricing from '@/vue-pages/pricing.vue'
import * as m from '../paraglide/messages.js'

const title = [Astro.locals.runtimeConfig.public.brand, m.pricing()].join(' | ')
const description = m.pricing_description()

const content = { title, description }
---

<Layout content={content}><VuePricing locale={Astro.locals.locale} client:load /></Layout>
