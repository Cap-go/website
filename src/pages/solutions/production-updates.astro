---
import Layout from '@/layouts/Layout.astro'
import * as m from '@/paraglide/messages'
import { getRelativeLocaleUrl } from 'astro:i18n'

const locale = Astro.locals.locale
const brand = Astro.locals.runtimeConfig.public.brand

const title = `${brand} | ${m.solutions_production_updates_title({}, { locale })}`
const description = m.solutions_production_updates_description({}, { locale })

const content = { title, description }
---

<Layout content={content}>
  <div>
    <!-- Hero Section with Problem Statement -->
    <section class="relative py-20 sm:py-28 lg:py-36 overflow-hidden">
      <div class="absolute inset-0 overflow-hidden pointer-events-none">
        <div class="absolute top-0 left-1/2 -translate-x-1/2 w-full max-w-6xl h-full">
          <div class="absolute top-1/4 left-1/4 w-96 h-96 bg-blue-500/10 rounded-full blur-3xl"></div>
          <div class="absolute bottom-1/4 right-1/4 w-96 h-96 bg-purple-500/10 rounded-full blur-3xl"></div>
        </div>
      </div>

      <div class="relative px-4 mx-auto max-w-7xl sm:px-6 lg:px-8">
        <div class="max-w-4xl mx-auto text-center">
          <div class="inline-flex items-center px-4 py-2 text-sm font-medium text-blue-200 border rounded-full border-blue-400/40 bg-blue-500/10 mb-8">
            <svg class="w-5 h-5 mr-2" fill="none" viewBox="0 0 24 24" stroke="currentColor">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M7 16a4 4 0 01-.88-7.903A5 5 0 1115.9 6L16 6a5 5 0 011 9.9M15 13l-3-3m0 0l-3 3m3-3v12" />
            </svg>
            {m.solutions_production_updates({}, { locale })}
          </div>

          <h1 class="text-4xl font-bold leading-tight text-white sm:text-5xl lg:text-6xl">
            {m.solutions_production_updates_hero_title({}, { locale })}
          </h1>

          <p class="max-w-3xl mx-auto mt-8 text-xl leading-relaxed text-gray-300">
            {m.solutions_production_updates_hero_subtitle({}, { locale })}
          </p>

          <div class="flex flex-col items-center justify-center gap-4 mt-10 sm:flex-row">
            <a
              href={getRelativeLocaleUrl(locale, 'register')}
              class="px-8 py-4 text-base font-semibold text-white transition-all duration-200 bg-blue-600 rounded-xl hover:bg-blue-700 focus:ring-2 focus:ring-blue-500 focus:ring-offset-2 shadow-lg shadow-blue-500/25"
            >
              {m.solutions_start_free_trial({}, { locale })}
            </a>
            <a
              href="https://cal.com/team/capgo/demo"
              class="px-8 py-4 text-base font-semibold text-gray-300 transition-all duration-200 border border-gray-600 rounded-xl hover:bg-gray-800 hover:text-white"
            >
              {m.solutions_talk_to_team({}, { locale })}
            </a>
          </div>

          <!-- Trust Badges -->
          <div class="flex flex-wrap items-center justify-center gap-6 mt-12 text-sm text-gray-400">
            <div class="flex items-center gap-2">
              <svg class="w-5 h-5 text-green-400" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
              </svg>
              {m.solutions_no_credit_card({}, { locale })}
            </div>
            <div class="flex items-center gap-2">
              <svg class="w-5 h-5 text-green-400" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
              </svg>
              {m.solutions_setup_minutes({}, { locale })}
            </div>
            <div class="flex items-center gap-2">
              <svg class="w-5 h-5 text-green-400" fill="currentColor" viewBox="0 0 20 20">
                <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
              </svg>
              {m.solutions_app_store_compliant({}, { locale })}
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- The Problem: Detailed Pain Point Section -->
    <section class="py-20 bg-gray-800">
      <div class="px-4 mx-auto max-w-7xl sm:px-6 lg:px-8">
        <div class="max-w-3xl mx-auto text-center mb-16">
          <h2 class="text-sm font-semibold tracking-wide text-red-400 uppercase mb-4">{m.solutions_the_problem({}, { locale })}</h2>
          <h3 class="text-3xl font-bold text-white sm:text-4xl">{m.solutions_production_problem_title({}, { locale })}</h3>
        </div>

        <div class="grid grid-cols-1 lg:grid-cols-2 gap-12 items-center">
          <!-- Problem Visualization -->
          <div class="bg-gray-900 rounded-2xl p-8 border border-red-500/30">
            <h4 class="text-xl font-bold text-white mb-6">{m.solutions_traditional_workflow({}, { locale })}</h4>

            <div class="space-y-6">
              <div class="flex items-start gap-4">
                <div class="flex-shrink-0 w-10 h-10 rounded-full bg-red-500/20 flex items-center justify-center">
                  <span class="text-red-400 font-bold">1</span>
                </div>
                <div>
                  <p class="font-semibold text-white">{m.solutions_problem_step1_title({}, { locale })}</p>
                  <p class="text-gray-400 text-sm mt-1">{m.solutions_problem_step1_desc({}, { locale })}</p>
                </div>
              </div>

              <div class="flex items-start gap-4">
                <div class="flex-shrink-0 w-10 h-10 rounded-full bg-red-500/20 flex items-center justify-center">
                  <span class="text-red-400 font-bold">2</span>
                </div>
                <div>
                  <p class="font-semibold text-white">{m.solutions_problem_step2_title({}, { locale })}</p>
                  <p class="text-gray-400 text-sm mt-1">{m.solutions_problem_step2_desc({}, { locale })}</p>
                </div>
              </div>

              <div class="flex items-start gap-4">
                <div class="flex-shrink-0 w-10 h-10 rounded-full bg-red-500/20 flex items-center justify-center">
                  <span class="text-red-400 font-bold">3</span>
                </div>
                <div>
                  <p class="font-semibold text-white">{m.solutions_problem_step3_title({}, { locale })}</p>
                  <p class="text-gray-400 text-sm mt-1">{m.solutions_problem_step3_desc({}, { locale })}</p>
                </div>
              </div>

              <div class="flex items-start gap-4">
                <div class="flex-shrink-0 w-10 h-10 rounded-full bg-red-500/20 flex items-center justify-center">
                  <span class="text-red-400 font-bold">4</span>
                </div>
                <div>
                  <p class="font-semibold text-white">{m.solutions_problem_step4_title({}, { locale })}</p>
                  <p class="text-gray-400 text-sm mt-1">{m.solutions_problem_step4_desc({}, { locale })}</p>
                </div>
              </div>
            </div>

            <div class="mt-8 p-4 bg-red-500/10 rounded-xl border border-red-500/30">
              <p class="text-red-300 font-medium">{m.solutions_problem_result({}, { locale })}</p>
            </div>
          </div>

          <!-- Real World Impact -->
          <div>
            <h4 class="text-xl font-bold text-white mb-6">{m.solutions_real_cost({}, { locale })}</h4>

            <div class="space-y-6">
              <div class="p-6 bg-gray-900 rounded-xl border border-gray-700">
                <div class="flex items-center gap-4 mb-3">
                  <div class="w-12 h-12 rounded-full bg-red-500/20 flex items-center justify-center">
                    <svg class="w-6 h-6 text-red-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z" />
                    </svg>
                  </div>
                  <div>
                    <p class="text-2xl font-bold text-white">3-7 {m.solutions_days({}, { locale })}</p>
                    <p class="text-gray-400">{m.solutions_avg_review_time({}, { locale })}</p>
                  </div>
                </div>
                <p class="text-gray-400 text-sm">{m.solutions_review_time_explanation({}, { locale })}</p>
              </div>

              <div class="p-6 bg-gray-900 rounded-xl border border-gray-700">
                <div class="flex items-center gap-4 mb-3">
                  <div class="w-12 h-12 rounded-full bg-red-500/20 flex items-center justify-center">
                    <svg class="w-6 h-6 text-red-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9.172 16.172a4 4 0 015.656 0M9 10h.01M15 10h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
                    </svg>
                  </div>
                  <div>
                    <p class="text-2xl font-bold text-white">23%</p>
                    <p class="text-gray-400">{m.solutions_user_churn({}, { locale })}</p>
                  </div>
                </div>
                <p class="text-gray-400 text-sm">{m.solutions_churn_explanation({}, { locale })}</p>
              </div>

              <div class="p-6 bg-gray-900 rounded-xl border border-gray-700">
                <div class="flex items-center gap-4 mb-3">
                  <div class="w-12 h-12 rounded-full bg-red-500/20 flex items-center justify-center">
                    <svg class="w-6 h-6 text-red-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8c-1.657 0-3 .895-3 2s1.343 2 3 2 3 .895 3 2-1.343 2-3 2m0-8c1.11 0 2.08.402 2.599 1M12 8V7m0 1v8m0 0v1m0-1c-1.11 0-2.08-.402-2.599-1M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
                    </svg>
                  </div>
                  <div>
                    <p class="text-2xl font-bold text-white">$47,000</p>
                    <p class="text-gray-400">{m.solutions_avg_cost_per_bug({}, { locale })}</p>
                  </div>
                </div>
                <p class="text-gray-400 text-sm">{m.solutions_bug_cost_explanation({}, { locale })}</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- The Solution: How Capgo Works -->
    <section class="py-20 bg-gray-900">
      <div class="px-4 mx-auto max-w-7xl sm:px-6 lg:px-8">
        <div class="max-w-3xl mx-auto text-center mb-16">
          <h2 class="text-sm font-semibold tracking-wide text-green-400 uppercase mb-4">{m.solutions_the_solution({}, { locale })}</h2>
          <h3 class="text-3xl font-bold text-white sm:text-4xl">{m.solutions_production_solution_title({}, { locale })}</h3>
          <p class="mt-4 text-xl text-gray-400">{m.solutions_production_solution_subtitle({}, { locale })}</p>
        </div>

        <!-- Solution Visualization -->
        <div class="bg-gray-800 rounded-2xl p-8 border border-green-500/30 max-w-4xl mx-auto mb-16">
          <h4 class="text-xl font-bold text-white mb-6 text-center">{m.solutions_with_capgo({}, { locale })}</h4>

          <div class="grid grid-cols-1 md:grid-cols-3 gap-6">
            <div class="text-center p-6 bg-gray-900 rounded-xl">
              <div class="w-16 h-16 rounded-full bg-green-500/20 flex items-center justify-center mx-auto mb-4">
                <span class="text-green-400 font-bold text-xl">1</span>
              </div>
              <p class="font-semibold text-white mb-2">{m.solutions_solution_step1_title({}, { locale })}</p>
              <p class="text-gray-400 text-sm">{m.solutions_solution_step1_desc({}, { locale })}</p>
              <p class="text-green-400 font-medium mt-3">{m.solutions_solution_step1_time({}, { locale })}</p>
            </div>

            <div class="text-center p-6 bg-gray-900 rounded-xl">
              <div class="w-16 h-16 rounded-full bg-green-500/20 flex items-center justify-center mx-auto mb-4">
                <span class="text-green-400 font-bold text-xl">2</span>
              </div>
              <p class="font-semibold text-white mb-2">{m.solutions_solution_step2_title({}, { locale })}</p>
              <p class="text-gray-400 text-sm">{m.solutions_solution_step2_desc({}, { locale })}</p>
              <p class="text-green-400 font-medium mt-3">{m.solutions_solution_step2_time({}, { locale })}</p>
            </div>

            <div class="text-center p-6 bg-gray-900 rounded-xl">
              <div class="w-16 h-16 rounded-full bg-green-500/20 flex items-center justify-center mx-auto mb-4">
                <span class="text-green-400 font-bold text-xl">3</span>
              </div>
              <p class="font-semibold text-white mb-2">{m.solutions_solution_step3_title({}, { locale })}</p>
              <p class="text-gray-400 text-sm">{m.solutions_solution_step3_desc({}, { locale })}</p>
              <p class="text-green-400 font-medium mt-3">{m.solutions_solution_step3_time({}, { locale })}</p>
            </div>
          </div>

          <div class="mt-8 p-4 bg-green-500/10 rounded-xl border border-green-500/30 text-center">
            <p class="text-green-300 font-medium">{m.solutions_solution_result({}, { locale })}</p>
          </div>
        </div>

        <!-- Code Example -->
        <div class="max-w-3xl mx-auto">
          <div class="p-6 bg-gray-800 border border-gray-700 rounded-2xl">
            <div class="flex items-center gap-2 mb-4">
              <div class="w-3 h-3 bg-red-500 rounded-full"></div>
              <div class="w-3 h-3 bg-yellow-500 rounded-full"></div>
              <div class="w-3 h-3 bg-green-500 rounded-full"></div>
              <span class="ml-2 text-sm text-gray-500">Terminal</span>
            </div>
            <pre class="overflow-x-auto text-sm text-gray-300"><code><span class="text-gray-500"># One-time setup (5 minutes)</span>
npm install @capgo/capacitor-updater
npx cap sync

<span class="text-gray-500"># Deploy an update (30 seconds)</span>
npm run build
npx @capgo/cli bundle upload

<span class="text-green-400">✔</span> Bundle uploaded successfully (2.3 MB)
<span class="text-green-400">✔</span> 47,382 devices will receive update on next launch
<span class="text-green-400">✔</span> Average rollout time: 4 hours

<span class="text-gray-500"># That's it. No app store. No review. No waiting.</span></code></pre>
          </div>
        </div>
      </div>
    </section>

    <!-- Real Business Scenario -->
    <section class="py-20 bg-gray-800">
      <div class="px-4 mx-auto max-w-7xl sm:px-6 lg:px-8">
        <div class="max-w-3xl mx-auto text-center mb-16">
          <h2 class="text-sm font-semibold tracking-wide text-blue-400 uppercase mb-4">{m.solutions_real_world({}, { locale })}</h2>
          <h3 class="text-3xl font-bold text-white sm:text-4xl">{m.solutions_scenario_title({}, { locale })}</h3>
        </div>

        <div class="bg-gray-900 rounded-2xl p-8 lg:p-12 border border-gray-700 max-w-5xl mx-auto">
          <div class="grid grid-cols-1 lg:grid-cols-2 gap-12">
            <!-- The Story -->
            <div>
              <div class="flex items-center gap-3 mb-6">
                <div class="w-12 h-12 bg-blue-500/20 rounded-full flex items-center justify-center">
                  <svg class="w-6 h-6 text-blue-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4" />
                  </svg>
                </div>
                <div>
                  <h4 class="text-xl font-bold text-white">{m.solutions_scenario_company({}, { locale })}</h4>
                  <p class="text-gray-400">{m.solutions_scenario_industry({}, { locale })}</p>
                </div>
              </div>

              <div class="space-y-6 text-gray-300">
                <p>{m.solutions_scenario_para1({}, { locale })}</p>
                <p>{m.solutions_scenario_para2({}, { locale })}</p>
                <p>{m.solutions_scenario_para3({}, { locale })}</p>
              </div>
            </div>

            <!-- The Results -->
            <div class="bg-gray-800 rounded-xl p-6">
              <h4 class="text-lg font-bold text-white mb-6">{m.solutions_scenario_results({}, { locale })}</h4>

              <div class="space-y-4">
                <div class="flex items-center justify-between p-4 bg-gray-900 rounded-lg">
                  <span class="text-gray-400">{m.solutions_scenario_metric1_label({}, { locale })}</span>
                  <span class="text-2xl font-bold text-green-400">{m.solutions_scenario_metric1_value({}, { locale })}</span>
                </div>
                <div class="flex items-center justify-between p-4 bg-gray-900 rounded-lg">
                  <span class="text-gray-400">{m.solutions_scenario_metric2_label({}, { locale })}</span>
                  <span class="text-2xl font-bold text-green-400">{m.solutions_scenario_metric2_value({}, { locale })}</span>
                </div>
                <div class="flex items-center justify-between p-4 bg-gray-900 rounded-lg">
                  <span class="text-gray-400">{m.solutions_scenario_metric3_label({}, { locale })}</span>
                  <span class="text-2xl font-bold text-green-400">{m.solutions_scenario_metric3_value({}, { locale })}</span>
                </div>
                <div class="flex items-center justify-between p-4 bg-gray-900 rounded-lg">
                  <span class="text-gray-400">{m.solutions_scenario_metric4_label({}, { locale })}</span>
                  <span class="text-2xl font-bold text-green-400">{m.solutions_scenario_metric4_value({}, { locale })}</span>
                </div>
              </div>

              <div class="mt-6 p-4 bg-blue-500/10 rounded-lg border border-blue-500/30">
                <p class="text-blue-300 text-sm italic">"{m.solutions_scenario_quote({}, { locale })}"</p>
                <p class="text-gray-400 text-sm mt-2">— {m.solutions_scenario_quote_author({}, { locale })}</p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- Key Benefits with Deep Explanation -->
    <section class="py-20 bg-gray-900">
      <div class="px-4 mx-auto max-w-7xl sm:px-6 lg:px-8">
        <div class="max-w-3xl mx-auto text-center mb-16">
          <h2 class="text-3xl font-bold text-white sm:text-4xl">{m.solutions_why_teams_choose({}, { locale })}</h2>
          <p class="mt-4 text-xl text-gray-400">{m.solutions_benefits_subtitle({}, { locale })}</p>
        </div>

        <div class="space-y-8">
          <!-- Benefit 1: Silent Updates -->
          <div class="bg-gray-800 rounded-2xl p-8 lg:p-10 border border-gray-700">
            <div class="grid grid-cols-1 lg:grid-cols-3 gap-8">
              <div class="lg:col-span-2">
                <div class="flex items-center gap-3 mb-4">
                  <div class="w-10 h-10 bg-blue-500/20 rounded-lg flex items-center justify-center">
                    <svg class="w-5 h-5 text-blue-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15.536 8.464a5 5 0 010 7.072m2.828-9.9a9 9 0 010 12.728M5.586 15.465a5 5 0 001.897-7.72m-3.732 9.143a9 9 0 010-12.728" />
                    </svg>
                  </div>
                  <h3 class="text-xl font-bold text-white">{m.solutions_benefit1_title({}, { locale })}</h3>
                </div>
                <p class="text-gray-300 mb-4">{m.solutions_benefit1_desc({}, { locale })}</p>
                <ul class="space-y-2">
                  <li class="flex items-start gap-2 text-gray-400">
                    <svg class="w-5 h-5 text-green-400 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                      <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
                    </svg>
                    {m.solutions_benefit1_point1({}, { locale })}
                  </li>
                  <li class="flex items-start gap-2 text-gray-400">
                    <svg class="w-5 h-5 text-green-400 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                      <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
                    </svg>
                    {m.solutions_benefit1_point2({}, { locale })}
                  </li>
                  <li class="flex items-start gap-2 text-gray-400">
                    <svg class="w-5 h-5 text-green-400 mt-0.5 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                      <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd" />
                    </svg>
                    {m.solutions_benefit1_point3({}, { locale })}
                  </li>
                </ul>
              </div>
              <div class="bg-gray-900 rounded-xl p-6 flex flex-col justify-center">
                <p class="text-4xl font-bold text-blue-400 mb-2">95%</p>
                <p class="text-gray-400">{m.solutions_benefit1_stat({}, { locale })}</p>
              </div>
            </div>
          </div>

          <!-- Benefit 2: Automatic Rollback -->
          <div class="bg-gray-800 rounded-2xl p-8 lg:p-10 border border-gray-700">
            <div class="grid grid-cols-1 lg:grid-cols-3 gap-8">
              <div class="lg:col-span-2">
                <div class="flex items-center gap-3 mb-4">
                  <div class="w-10 h-10 bg-purple-500/20 rounded-lg flex items-center justify-center">
                    <svg class="w-5 h-5 text-purple-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 10h10a8 8 0 018 8v2M3 10l6 6m-6-6l6-6" />
                    </svg>
                  </div>
                  <h3 class="text-xl font-bold text-white">{m.solutions_benefit2_title({}, { locale })}</h3>
