---
import Layout from '@/layouts/Layout.astro'
import VueTopcapacitorapp from '@/vue-pages/top_capacitor_app.vue'
import * as m from "../paraglide/messages.js"

const title = [Astro.locals.runtimeConfig.public.brand, m.top_capacitor_apps()].join(' | ')
const description = m.top_100_app_using_capacitor_on_android_play_store()

const content = { title, description }
---

<Layout content={content}><VueTopcapacitorapp locale={Astro.locals.locale} client:load /></Layout>
