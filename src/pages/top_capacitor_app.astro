---
import Layout from '@/layouts/Layout.astro'
import { defaultLocale, type Locales } from '@/services/locale'
import VueTopcapacitorapp from '@/vue-pages/top_capacitor_app.vue'

const title = 'Capgo | Top Capacitor apps'
const description = 'List of top 100 app using Capacitor on android play store'

const content = {
  description,
  title,
}
---

<Layout content={content}><VueTopcapacitorapp locale={(Astro.currentLocale || defaultLocale) as Locales} client:load /></Layout>
