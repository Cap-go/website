---
import Layout from '@/layouts/Layout.astro'
import VueTopcordovaapp from '@/vue-pages/top_cordova_app.vue'
import translations from '@/services/translations'

const title = [Astro.locals.runtimeConfig.public.brand, translations['top_cordova_apps_title'][Astro.locals.locale]].join(' | ')
const description = translations['top_cordova_apps'][Astro.locals.locale]

const content = { title, description }
---

<Layout content={content}><VueTopcordovaapp locale={Astro.locals.locale} client:load /></Layout>
