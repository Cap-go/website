---
import VueDataPolicy from '@/vue-pages/data_policy.vue'
import Layout from '@/layouts/Layout.astro'

const title = [Astro.locals.runtimeConfig.public.brand, Astro.locals.translations['dp'][Astro.locals.locale]].join(' | ')
const description = Astro.locals.translations['dp'][Astro.locals.locale]

const content = { title, description }
---

<Layout content={content}><VueDataPolicy locale={Astro.locals.locale} client:load /></Layout>
