---
import Layout from '@/layouts/Layout.astro'
import VueReturn from '@/vue-pages/return.vue'
import * as m from '../paraglide/messages.js'

const title = [Astro.locals.runtimeConfig.public.brand, m.return_policy()].join(' | ')
const description = m.read_our_return_policy()

const content = { title, description }
---

<Layout content={content}><VueReturn locale={Astro.locals.locale} client:load /></Layout>
