---
import Layout from '@/layouts/Layout.astro'
import VuePrivacy from '@/vue-pages/privacy.vue'
import * as m from '../paraglide/messages.js'

const title = [Astro.locals.runtimeConfig.public.brand, m.privacy_policy()].join(' | ')
const description = m.read_our_privacy_policy()

const content = { title, description }
---

<Layout content={content}><VuePrivacy locale={Astro.locals.locale} client:load /></Layout>
