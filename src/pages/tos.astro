---
import VueTos from '@/vue-pages/tos.vue'
import Layout from '@/layouts/Layout.astro'
import * as m from "../paraglide/messages.js"

const title = [Astro.locals.runtimeConfig.public.brand, m.terms()].join(' | ')
const description = m.read_our_terms_of_service()

const content = { title, description }
---

<Layout content={content}><VueTos locale={Astro.locals.locale} client:load /></Layout>
