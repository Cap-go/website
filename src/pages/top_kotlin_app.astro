---
import Layout from '@/layouts/Layout.astro'
import { defaultLocale } from '@/services/locale'
import VueTopkotlinapp from '@/vue-pages/top_kotlin_app.vue'

const title = 'Capgo | Top Kotlin apps'
const description = 'List of top 100 app using Kotlin on android store'

const content = {
  description,
  title,
}
---

<Layout content={content}><VueTopkotlinapp locale={Astro.currentLocale || defaultLocale} client:load /></Layout>
