---
import Layout from '@/layouts/Layout.astro'
import VueTopkotlinapp from '@/vue-pages/top_kotlin_app.vue'
import * as m from '../paraglide/messages.js'

const title = [Astro.locals.runtimeConfig.public.brand, m.top_kotlin_apps_title({}, { locale: Astro.locals.locale })].join(' | ')
const description = m.top_kotlin_apps({}, { locale: Astro.locals.locale })

const content = { title, description }
---

<Layout content={content}><VueTopkotlinapp locale={Astro.locals.locale} client:load /></Layout>
