---
import VueDpa from '@/vue-pages/dpa.vue'
import Layout from '@/layouts/Layout.astro'
import * as m from '../paraglide/messages.js'

const title = [Astro.locals.runtimeConfig.public.brand, m.data_processing_agreement()].join(' | ')
const description = m.read_our_data_processing_agreement()

const content = { title, description }
---

<Layout content={content}><VueDpa locale={Astro.locals.locale} client:load /></Layout>
