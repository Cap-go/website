---
import Layout from '@/layouts/Layout.astro'
import translations from '@/services/translations'
import VueTopnativescriptapp from '@/vue-pages/top_native_script_app.vue'

const brand = Astro.locals.runtimeConfig.public.brand

const title = [brand, translations['top_native_script_apps_title'][Astro.locals.locale]].join(' | ')
const description = translations['top_native_script_apps'][Astro.locals.locale]

const content = { title, description }
---

<Layout content={content}><VueTopnativescriptapp locale={Astro.locals.locale} client:load /></Layout>
