---
import Layout from '@/layouts/Layout.astro'
import VueTopflutterapp from '@/vue-pages/top_flutter_app.vue'
import translations from '@/services/translations'

const brand = Astro.locals.runtimeConfig.public.brand

const title = [brand, translations['top_flutter_apps_title'][Astro.locals.locale]].join(' | ')
const description = translations['top_flutter_apps'][Astro.locals.locale]

const content = { title, description }
---

<Layout content={content}><VueTopflutterapp locale={Astro.locals.locale} client:load /></Layout>
