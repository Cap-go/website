---
import Layout from '@/layouts/Layout.astro'
import VueTopcapacitorapp from '@/vue-pages/top_capacitor_app.vue'

const title = [Astro.locals.runtimeConfig.public.brand, Astro.locals.translations['top_capacitor_apps'][Astro.locals.locale]].join(' | ')
const description = Astro.locals.translations['top_100_app_using_capacitor_on_android_play_store'][Astro.locals.locale]

const content = { title, description }
---

<Layout content={content}><VueTopcapacitorapp locale={Astro.locals.locale} client:load /></Layout>
