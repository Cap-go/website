---
import Layout from '@/layouts/Layout.astro'
import VueTrust from '@/vue-pages/trust.vue'
import * as m from "../paraglide/messages.js"

const title = [Astro.locals.runtimeConfig.public.brand, m.trust()].join(' | ')
const description = m.consulting_description()

const content = { title, description }
---

<Layout content={content}><VueTrust locale={Astro.locals.locale} client:load /></Layout>
